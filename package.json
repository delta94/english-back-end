{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run watch",
    "add-migration": "typeorm migration:generate -n",
    "run-migrations": "typeorm migration:run",
    "serverless": "serverless offline --websocketPort 3002 --httpPort 3000 --lambdaPort 3001 --noAuth --noTimeout --printOutput",
    "generate-schema": "ts-node ./src/utils/generateSchemaFile.ts",
    "clean": "rimraf dist/*",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "clean lint tsc install-youtubedl",
    "watch": "tsc-watch --onSuccess \"npm run migandstart\" --compiler typescript/bin/tsc",
    "migandstart": "typeorm migration:run && node dist/index",
    "install-youtubedl": "ts-node scripts/install-youtubedl.ts",
    "start:prod": "typeorm migration:run && node dist/index",
    "testdb:setup": "ts-node ./src/test-utils/setup.ts",
    "test": "npm run testdb:setup && jest",
    "test:watch": "npm run testdb:setup && jest --watch"
  },
  "keywords": [],
  "author": "tingadev",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/request": "^2.48.5",
    "apollo-server": "^2.16.1",
    "apollo-server-errors": "^2.4.2",
    "aws-sdk": "^2.750.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "cloudinary": "^1.23.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-healthcheck": "^0.1.0",
    "express-http-context": "^1.2.4",
    "graphql": "^15.3.0",
    "imgix-core-js": "^2.3.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "request": "^2.88.2",
    "shortid": "^2.2.16",
    "tsc-watch": "^4.2.9",
    "type-graphql": "^1.0.0-rc.3",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^3.9.7",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.158",
    "@types/multer": "^1.4.4",
    "@types/on-finished": "^2.3.1",
    "@types/on-headers": "^1.0.0",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.3.0",
    "@types/zxcvbn": "^4.4.0"
  }
